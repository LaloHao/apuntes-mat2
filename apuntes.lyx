\begin_layout Subsection
Vectores en el espacio
\end_layout

\begin_layout Standard
Sean 
\begin_inset Formula $u=\left\langle u_{1},u_{2},u_{3}\right\rangle $
\end_inset

 y 
\begin_inset Formula $v=\left\langle v_{1},v_{2},v_{3}\right\rangle $
\end_inset

 vectores en el espacio y sea 
\begin_inset Formula $c$
\end_inset

 un escalar
\end_layout

\begin_layout Enumerate
Igualdad de vectores
\begin_inset Newline newline
\end_inset

 
\begin_inset Formula $u=v\Leftrightarrow u_{1}=v_{1},u_{2}=v_{2},u_{3}=v_{3}$
\end_inset


\end_layout

\begin_layout Enumerate
Longitud
\begin_inset Newline newline
\end_inset


\begin_inset Formula $\mbox{\|v\|=\sqrt{v_{1}^{2}+v_{2}^{2}+v_{3}^{2}}}$
\end_inset


\end_layout

\begin_layout Enumerate
Vector unitario en dirección 
\begin_inset Formula $v$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $u=\frac{1}{\|v\|}\left\langle v_{1},v_{2},v_{3}\right\rangle $
\end_inset


\end_layout

\begin_layout Enumerate
Suma de vectores
\begin_inset Newline newline
\end_inset


\begin_inset Formula $v+u=\left\langle v_{1}+u_{1},v_{2}+u_{2},v_{3}+u_{3}\right\rangle $
\end_inset


\end_layout

\begin_layout Enumerate
Producto por un escalar
\begin_inset Newline newline
\end_inset


\begin_inset Formula $cv=\left\langle cv_{1},cv_{2},cv_{3}\right\rangle $
\end_inset


\end_layout

\begin_layout Enumerate
Vectores paralelos
\begin_inset Newline newline
\end_inset

Dos vectores no nulos 
\begin_inset Formula $u$
\end_inset

 y 
\begin_inset Formula $v$
\end_inset

 son paralelos si existe algún escalar 
\begin_inset Formula $c$
\end_inset

 tal que 
\begin_inset Formula $u=cv$
\end_inset


\end_layout

\begin_layout Enumerate
Producto punto/escalar
\begin_inset Newline newline
\end_inset


\begin_inset Formula $u\cdot v=u_{1}v_{1}+u_{2}v_{2}+u_{3}v_{3}$
\end_inset


\end_layout

\begin_layout Enumerate
Vectores ortogonales
\begin_inset Newline newline
\end_inset


\begin_inset Formula $u$
\end_inset

 y 
\begin_inset Formula $v$
\end_inset

 son ortogonales si 
\begin_inset Formula $u\cdot v=0$
\end_inset


\end_layout

\begin_layout Enumerate
Angulo entre vectores
\begin_inset Newline newline
\end_inset

El angulo entre 
\begin_inset Formula $u$
\end_inset

 y 
\begin_inset Formula $v$
\end_inset

 se define como 
\begin_inset Formula $cos\mbox{\left(\theta\right)=}\frac{u\cdot v}{\|u\|\|v\|}$
\end_inset


\end_layout

\begin_layout Enumerate
Desigualdad triangular
\begin_inset Newline newline
\end_inset


\begin_inset Formula $\|u+v\|\leq\|u\|+\|v\|$
\end_inset


\end_layout

\begin_layout Enumerate
Proyección de 
\begin_inset Formula $u$
\end_inset

 sobre 
\begin_inset Formula $v$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $proy_{v}u=\frac{u\cdot v}{\|v\|^{2}}v$
\end_inset


\end_layout

